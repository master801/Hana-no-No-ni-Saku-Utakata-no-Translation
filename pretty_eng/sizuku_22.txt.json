      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「同じですよ。ずっと二人で過ごしてきたんですから。\\nそれに俺がこんな目じゃなかったら、閉じ込められる事も\\nなかった訳ですし」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「同じですよ。ずっと二人で過ごしてきたんですから。それに俺がこんな目じゃなかったら、閉じ込められる事もなかった訳ですし」",
      "txt_idk_2": "「同じですよ。ずっと二人で過ごしてきたんですから。それに俺がこんな目じゃなかったら、閉じ込められる事もなかった訳ですし」"
    },
    {
      "actual_character_speaking": "雫",
      "hidden_character_speaking": null,
      "txt": "「そうね……そうかもしれない。\\nじゃあ、そこはお互い様ということで」",
      "voices": [
        {
          "name": "雫",
          "pan": 0,
          "type": 0,
          "voice": "a05_30995"
        }
      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「そうね……そうかもしれない。じゃあ、そこはお互い様ということで」",
      "txt_idk_2": "「そうね……そうかもしれない。じゃあ、そこはお互い様ということで」"
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「ええ」",
