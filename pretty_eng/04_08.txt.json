      "voices": [
        {
          "name": "麗奈",
          "pan": 0,
          "type": 0,
          "voice": "a06_00525"
        }
      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「ここは具体的な物が何一つないのよ。他のはまだ中身が想像できるんだけど……」",
      "txt_idk_2": "「ここは具体的な物が何一つないのよ。他のはまだ中身が想像できるんだけど……」"
    },
    {
