      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「願いが叶うってやつだろ？」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "麗奈",
      "hidden_character_speaking": null,
      "txt": "「そうじゃなくて……そもそも、どうやったら願いが叶うの？\\nそれに制限はあるの？　叶えてくれる数は？」",
      "voices": [
        {
          "name": "麗奈",
          "pan": 0,
          "type": 0,
          "voice": "a06_00524"
        }
      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「そうじゃなくて……そもそも、どうやったら願いが叶うの？それに制限はあるの？　叶えてくれる数は？」",
      "txt_idk_2": "「そうじゃなくて……そもそも、どうやったら願いが叶うの？それに制限はあるの？　叶えてくれる数は？」"
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "そこまで一息でいって、ため息をついた。",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "麗奈",
      "hidden_character_speaking": null,
