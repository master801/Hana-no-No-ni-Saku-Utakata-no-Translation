      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "冗談ともつかない返事がくる。",
