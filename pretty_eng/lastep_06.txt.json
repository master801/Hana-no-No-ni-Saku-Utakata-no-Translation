      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「……だよなぁ」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「そこで道隆には色を見て貰おうと思っている」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50703"
        }
      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「いいけど、それなら普段から――」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "言いかけて理解した。",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "桜花がわざわざ言うのだから、普通に見ている物じゃない。",
