      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「風呂に入ってくる。\\n桜花も早く休んだ方がいいぞ」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「風呂に入ってくる。桜花も早く休んだ方がいいぞ」",
      "txt_idk_2": "「風呂に入ってくる。桜花も早く休んだ方がいいぞ」"
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「そうだね……横になって寝る努力くらいはしてみようかな」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50705"
