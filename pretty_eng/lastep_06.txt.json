      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「やぁ、お帰り」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50692"
