      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「そりゃ……鏡石があったっていう、昔無くなった社の跡地を\\n……って、まさか」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「そりゃ……鏡石があったっていう、昔無くなった社の跡地を……って、まさか」",
      "txt_idk_2": "「そりゃ……鏡石があったっていう、昔無くなった社の跡地を……って、まさか」"
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「社の跡を見に行って、その場所であんな現象が起きていた。\\n関係が無いとは言い切れない」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50699"
        }
      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「社の跡を見に行って、その場所であんな現象が起きていた。関係が無いとは言い切れない」",
      "txt_idk_2": "「社の跡を見に行って、その場所であんな現象が起きていた。関係が無いとは言い切れない」"
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「でも、まさか」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「それも今さらの話だよ」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50700"
