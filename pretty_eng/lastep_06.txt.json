      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "……本当にその通りだ。",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「具体的にはどうするんだ？」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「……さあ……それが分からない。\\nもう一度近寄って良い物かどうかすらも分からないからね」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50701"
        }
      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「……さあ……それが分からない。もう一度近寄って良い物かどうかすらも分からないからね」",
      "txt_idk_2": "「……さあ……それが分からない。もう一度近寄って良い物かどうかすらも分からないからね」"
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「ただ、確認はしなくちゃいけない」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50702"
