      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「じゃあ、お願いしようかな。\\nでもろくに寝てないから、エロい事はなしでな」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「じゃあ、お願いしようかな。でもろくに寝てないから、エロい事はなしでな」",
