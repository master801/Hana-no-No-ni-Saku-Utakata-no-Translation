      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "雫",
      "hidden_character_speaking": null,
      "txt": "「おはよう。……昨日はごめんなさい」",
