      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「俺にとっては！」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "雫",
      "hidden_character_speaking": null,
      "txt": "「変なの」",
      "voices": [
        {
          "name": "雫",
          "pan": 0,
          "type": 0,
          "voice": "a05_30392"
        }
      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "くすりと笑う。\\nそれでやっと緊張がほぐれた。",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "くすりと笑う。それでやっと緊張がほぐれた。",
      "txt_idk_2": "くすりと笑う。それでやっと緊張がほぐれた。"
    },
    {
      "actual_character_speaking": "雫",
      "hidden_character_speaking": null,
