      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "雫",
      "hidden_character_speaking": null,
      "txt": "「放課後かしらね……。\\n少し時間を貰っていい？」",
      "voices": [
        {
          "name": "雫",
          "pan": 0,
          "type": 0,
          "voice": "a05_30394"
        }
      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「放課後かしらね……。少し時間を貰っていい？」",
      "txt_idk_2": "「放課後かしらね……。少し時間を貰っていい？」"
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
