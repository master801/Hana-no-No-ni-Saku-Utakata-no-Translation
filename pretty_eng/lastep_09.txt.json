      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「ただ、嘘はついていないというのだけは最初に言う。\\n突拍子も無いかも知れないけれど、俺たちが見た内容を\\n正直に言う」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
