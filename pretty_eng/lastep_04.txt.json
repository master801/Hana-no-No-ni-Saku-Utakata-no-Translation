      "idk_1": 456,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "……木々の葉を散らす音を立てた。",
      "voices": null,
      "idk_1": 456,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「――！？」",
      "voices": null,
      "idk_1": 456,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「……今のではっきりしたね。\\nあの中には普通に樹が生えている。飲み込まれたんじゃない。\\nただ、ああなっていて見えないだけなんだ」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50686"
        }
      ],
      "idk_1": 456,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「……今のではっきりしたね。あの中には普通に樹が生えている。飲み込まれたんじゃない。ただ、ああなっていて見えないだけなんだ」",
      "txt_idk_2": "「……今のではっきりしたね。あの中には普通に樹が生えている。飲み込まれたんじゃない。ただ、ああなっていて見えないだけなんだ」"
    },
