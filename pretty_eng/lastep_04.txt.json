      ],
      "idk_1": 456,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「だよな。あの黒いの……穴っぽい感じだから、もしも中に\\n入ったらどこに出てくるのか分からないし」",
      "voices": null,
      "idk_1": 456,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「だよな。あの黒いの……穴っぽい感じだから、もしも中に入ったらどこに出てくるのか分からないし」",
      "txt_idk_2": "「だよな。あの黒いの……穴っぽい感じだから、もしも中に入ったらどこに出てくるのか分からないし」"
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「……物を吸い込んだりは、しないと思う」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50684"
