      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "中を見て、また蓋を閉じた。",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "神主",
      "hidden_character_speaking": null,
      "txt": "「見たのは何十年ぶりになるかな……。\\nどういった経緯の物かは、手紙にあった通りでいいのかな」",
      "voices": [
        {
          "name": "神主",
          "pan": 0,
          "type": 0,
          "voice": "c03_30005"
        }
      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「見たのは何十年ぶりになるかな……。どういった経緯の物かは、手紙にあった通りでいいのかな」",
      "txt_idk_2": "「見たのは何十年ぶりになるかな……。どういった経緯の物かは、手紙にあった通りでいいのかな」"
    },
    {
      "actual_character_speaking": "雫",
      "hidden_character_speaking": null,
      "txt": "「はい」",
