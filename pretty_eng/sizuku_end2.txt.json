      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "神主",
      "hidden_character_speaking": null,
      "txt": "「おや、早かったね」",
      "voices": [
        {
          "name": "神主",
          "pan": 0,
          "type": 0,
          "voice": "c03_30001"
