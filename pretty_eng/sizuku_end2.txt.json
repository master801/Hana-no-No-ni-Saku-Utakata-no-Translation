      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「……その鏡って、結局なんだったんですか？」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "神主",
      "hidden_character_speaking": null,
      "txt": "「ふむ……」",
      "voices": [
        {
          "name": "神主",
          "pan": 0,
          "type": 0,
          "voice": "c03_30009"
