      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "神主さんは石段の下で俺たちを待っていたようだ。",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「こんにちは」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "雫",
      "hidden_character_speaking": null,
      "txt": "「今日はありがとうございます」",
      "voices": [
        {
          "name": "雫",
          "pan": 0,
          "type": 0,
          "voice": "a05_31061"
