      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "授業ではよく使う場所だが、休日では空気も変わってる\\nように感じてしまう。",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "授業ではよく使う場所だが、休日では空気も変わってるように感じてしまう。",
      "txt_idk_2": "授業ではよく使う場所だが、休日では空気も変わってるように感じてしまう。"
    },
    {
      "actual_character_speaking": "雫",
      "hidden_character_speaking": null,
      "txt": "「……少し待ちましょうか」",
      "voices": [
        {
          "name": "雫",
          "pan": 0,
          "type": 0,
          "voice": "a05_30272"
