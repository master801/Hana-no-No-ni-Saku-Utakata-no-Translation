      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "恋",
      "idk_1": null,
      "txt": "「新堂君……また悩み事ですか？」",
      "voices": [
        {
          "name": "恋",
          "pan": 0,
          "type": 0,
          "voice": "b04_50003"
