      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": null,
      "idk_1": null,
      "txt": "麗奈の部屋に何度も出入りしている、麗奈じゃない色。",
