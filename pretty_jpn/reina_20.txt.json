      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "汐音",
      "idk_1": null,
      "txt": "「どっちに全力を出すかだよね」",
      "voices": [
        {
          "name": "汐音",
          "pan": 0,
          "type": 0,
          "voice": "a03_20118"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "麗奈",
      "idk_1": null,
      "txt": "「そもそも、全力出す必要があるの？　怪我してるなら\\n治療でいいじゃない」",
      "voices": [
        {
          "name": "麗奈",
          "pan": 0,
          "type": 0,
          "voice": "a06_20601"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「そもそも、全力出す必要があるの？　怪我してるなら治療でいいじゃない」",
      "txt_idk_2": "「そもそも、全力出す必要があるの？　怪我してるなら治療でいいじゃない」"
    },
    {
      "character_speaking": "汐音",
      "idk_1": null,
      "txt": "「それもそうだよね」",
      "voices": [
        {
          "name": "汐音",
          "pan": 0,
          "type": 0,
          "voice": "a03_20119"
        }
