      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "汐音",
      "idk_1": null,
      "txt": "「それは？」",
      "voices": [
        {
          "name": "汐音",
          "pan": 0,
          "type": 0,
          "voice": "a03_20126"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "麗奈",
      "idk_1": null,
      "txt": "「うちの掃除。大掃除の手伝い」",
      "voices": [
        {
