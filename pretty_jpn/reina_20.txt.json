      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「――よし！　分かった！\\nありがとう」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「――よし！　分かった！ありがとう」",
      "txt_idk_2": "「――よし！　分かった！ありがとう」"
    },
    {
      "character_speaking": "汐音",
      "idk_1": null,
      "txt": "「いきなり元気になった……」",
