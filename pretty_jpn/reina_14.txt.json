      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "麗奈",
      "idk_1": null,
      "txt": "「……？　ええ、そうだけど」",
      "voices": [
        {
          "name": "麗奈",
          "pan": 0,
