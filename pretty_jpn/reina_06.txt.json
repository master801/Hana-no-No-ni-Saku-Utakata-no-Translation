      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「……わかった。預かる。\\n中も見ないで渡すよ」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「……わかった。預かる。中も見ないで渡すよ」",
