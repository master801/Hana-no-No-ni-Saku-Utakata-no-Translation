      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「何でも『うちのは本物』らしい。\\nそれがどういう意味か分からないから、じゃあ見に行こうって\\n話になった」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「何でも『うちのは本物』らしい。それがどういう意味か分からないから、じゃあ見に行こうって話になった」",
      "txt_idk_2": "「何でも『うちのは本物』らしい。それがどういう意味か分からないから、じゃあ見に行こうって話になった」"
    },
    {
      "character_speaking": "美奈",
