      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": null,
      "idk_1": null,
      "txt": "色っぽい仕草に、俺の方も硬くなってきてしまっていた。",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "涼子",
      "idk_1": null,
      "txt": "「せん、ぱい……ちゅ……んん、……ぁ……ぴちゃ……」",
