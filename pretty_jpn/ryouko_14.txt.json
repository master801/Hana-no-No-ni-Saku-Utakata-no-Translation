        }
      ],
      "idk_2": 456,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「いや、今日はそのままで」",
