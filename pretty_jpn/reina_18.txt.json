      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "桜花",
      "idk_1": null,
      "txt": "「……なるほどね」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
