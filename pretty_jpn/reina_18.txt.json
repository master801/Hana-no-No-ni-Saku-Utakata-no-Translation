      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「思い付きで期待させてしまって悪い」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "麗奈",
      "idk_1": null,
      "txt": "「構わないわ。ありそうだと私も思ったもの」",
