      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "麗奈",
      "idk_1": null,
      "txt": "「その割に、道隆が辛気くさい顔してるのが気にいらないわ」",
