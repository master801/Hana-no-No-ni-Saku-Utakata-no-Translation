      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「けれどあまりにもヒントが無さ過ぎるだろ。\\n桜花は麗奈の家を見た事もないんだし」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「けれどあまりにもヒントが無さ過ぎるだろ。桜花は麗奈の家を見た事もないんだし」",
      "txt_idk_2": "「けれどあまりにもヒントが無さ過ぎるだろ。桜花は麗奈の家を見た事もないんだし」"
    },
    {
      "character_speaking": "桜花",
      "idk_1": null,
      "txt": "「いや……手紙の件を考えれば、ある程度予測は立てられる」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_20241"
        }
