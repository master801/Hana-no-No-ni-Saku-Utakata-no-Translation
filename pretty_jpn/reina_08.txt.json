      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "千歳",
      "idk_1": null,
      "txt": "「……そうね……」",
      "voices": [
        {
          "name": "千歳",
          "pan": 0,
          "type": 0,
          "voice": "b01_20016"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": null,
      "idk_1": null,
      "txt": "千歳さんの表情も悩ましげだ。",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "千歳",
      "idk_1": null,
      "txt": "「これだけ大きな家だと、固定資産税も掛かってしまうし\\n使わないなら手放した方がいいけれど、そうなると今度は……」",
      "voices": [
        {
          "name": "千歳",
          "pan": 0,
          "type": 0,
          "voice": "b01_20017"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「これだけ大きな家だと、固定資産税も掛かってしまうし使わないなら手放した方がいいけれど、そうなると今度は……」",
      "txt_idk_2": "「これだけ大きな家だと、固定資産税も掛かってしまうし使わないなら手放した方がいいけれど、そうなると今度は……」"
    },
    {
      "character_speaking": "桜花",
      "idk_1": null,
      "txt": "「ボクの事は気にしないで貰って構わないけれどね。\\nいざとなったら道隆の実家でお世話になる」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_20128"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「ボクの事は気にしないで貰って構わないけれどね。いざとなったら道隆の実家でお世話になる」",
      "txt_idk_2": "「ボクの事は気にしないで貰って構わないけれどね。いざとなったら道隆の実家でお世話になる」"
    },
    {
      "character_speaking": "千歳",
      "idk_1": null,
      "txt": "「そうもいかないでしょう」",
      "voices": [
        {
          "name": "千歳",
          "pan": 0,
          "type": 0,
          "voice": "b01_20018"
