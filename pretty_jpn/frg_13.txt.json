      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": null,
      "idk_1": null,
      "txt": "包丁を使うのが苦手だから、まずはこういう所から努力を\\nしていくらしい。",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "包丁を使うのが苦手だから、まずはこういう所から努力をしていくらしい。",
      "txt_idk_2": "包丁を使うのが苦手だから、まずはこういう所から努力をしていくらしい。"
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「……うん。美味い」",
