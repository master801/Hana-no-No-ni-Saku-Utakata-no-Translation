      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「……なるほど。\\nメンズ物まであるのはそのためか」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「……なるほど。メンズ物まであるのはそのためか」",
      "txt_idk_2": "「……なるほど。メンズ物まであるのはそのためか」"
    },
