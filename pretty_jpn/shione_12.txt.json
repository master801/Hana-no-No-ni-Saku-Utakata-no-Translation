      "txt": "「ああ……あれか。\\n笑っちゃうよな」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「ああ……あれか。笑っちゃうよな」",
