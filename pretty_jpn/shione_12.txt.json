      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "和泉",
      "idk_1": null,
      "txt": "「汐音～～～。おはよう～～」",
      "voices": [
