      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「アナタ、ココの、セイトサン、ですね？\\nチョット、オハナシを」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「アナタ、ココの、セイトサン、ですね？チョット、オハナシを」",
      "txt_idk_2": "「アナタ、ココの、セイトサン、ですね？チョット、オハナシを」"
    },
    {
      "character_speaking": "和泉",
      "idk_1": null,
      "txt": "「はいはいー。何でも聞いてくださいねー。\\nあ、テレビ映ってます？　カメラどこ？」",
      "voices": [
        {
          "name": "和泉",
          "pan": 0,
