      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "桜花",
      "idk_1": null,
      "txt": "「そちらも似たような事になっているけれどね。\\nそろそろ直さないと、また汐音に嫌がられるよ」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_30158"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「そちらも似たような事になっているけれどね。そろそろ直さないと、また汐音に嫌がられるよ」",
