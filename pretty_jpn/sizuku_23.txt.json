      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "桜花",
      "idk_1": null,
      "txt": "「やあ、お帰り。\\n雫もいらっしゃい。よく来たね」",
