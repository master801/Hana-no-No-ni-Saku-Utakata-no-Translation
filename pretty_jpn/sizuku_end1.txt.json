      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "涼子",
      "idk_1": null,
      "txt": "「おはようございます。どなたかお待ちですか？」",
      "voices": [
