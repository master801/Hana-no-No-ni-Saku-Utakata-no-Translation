      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "桜花",
      "idk_1": null,
      "txt": "「そっか、そこまで分かってるなら何も言わないよ。\\n次にすべき事を考えるとしよう」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_30171"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「そっか、そこまで分かってるなら何も言わないよ。次にすべき事を考えるとしよう」",
      "txt_idk_2": "「そっか、そこまで分かってるなら何も言わないよ。次にすべき事を考えるとしよう」"
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「次に？」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "桜花",
      "idk_1": null,
