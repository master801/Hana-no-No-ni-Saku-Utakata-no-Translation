      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「そうですね……それが正しいかもしれないです。\\nあるいは準備をして、いずれまたの機会を待つとか」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「そうですね……それが正しいかもしれないです。あるいは準備をして、いずれまたの機会を待つとか」",
      "txt_idk_2": "「そうですね……それが正しいかもしれないです。あるいは準備をして、いずれまたの機会を待つとか」"
    },
    {
      "character_speaking": "雫",
      "idk_1": null,
      "txt": "「ええ」",
      "voices": [
        {
          "name": "雫",
          "pan": 0,
