      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "優枝",
      "idk_1": null,
      "txt": "「はい……」",
      "voices": [
        {
          "name": "優枝",
          "pan": 0,
          "type": 0,
          "voice": "b02_30014"
