      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「そうだけど……美緒ちゃんも？」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "美緒",
      "idk_1": null,
      "txt": "「そうですっ」",
      "voices": [
        {
          "name": "美緒",
