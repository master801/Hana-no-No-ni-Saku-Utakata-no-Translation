      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "優枝",
      "idk_1": null,
      "txt": "「ですがここの子はいつもいますから、外で遊ぶ時はこっちに\\n来るのも良いと思いますよ」",
      "voices": [
        {
          "name": "優枝",
          "pan": 0,
          "type": 0,
          "voice": "b02_40005"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「ですがここの子はいつもいますから、外で遊ぶ時はこっちに来るのも良いと思いますよ」",
      "txt_idk_2": "「ですがここの子はいつもいますから、外で遊ぶ時はこっちに来るのも良いと思いますよ」"
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「公園も外に子猫沢山いるしね」",
