      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": null,
      "idk_1": null,
      "txt": "美緒ちゃんが猫を受け取る。",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "美緒",
      "idk_1": null,
      "txt": "「おっきいですね。兄弟なのにうちよりずっとおっきいです」",
      "voices": [
        {
          "name": "美緒",
          "pan": 0,
          "type": 0,
          "voice": "b06_40029"
