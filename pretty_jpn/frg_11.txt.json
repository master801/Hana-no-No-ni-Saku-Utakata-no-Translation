      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": null,
      "idk_1": null,
      "txt": "笑顔で言う優枝さんはとても楽しそうだ。",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
