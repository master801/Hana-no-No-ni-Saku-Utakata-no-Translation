      ],
      "idk_2": 456,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「これは不可抗力だ！　彼女とこうやって密着してたら\\n健全な男ならこうなるんだ！！」",
      "voices": null,
      "idk_2": 456,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「これは不可抗力だ！　彼女とこうやって密着してたら健全な男ならこうなるんだ！！」",
