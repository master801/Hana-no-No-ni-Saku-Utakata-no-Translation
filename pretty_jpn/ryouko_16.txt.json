      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「例えだよ、例え。\\nでも気持ちは分かってくれるだろう？」",
      "voices": null,
      "idk_2": 456,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「例えだよ、例え。でも気持ちは分かってくれるだろう？」",
