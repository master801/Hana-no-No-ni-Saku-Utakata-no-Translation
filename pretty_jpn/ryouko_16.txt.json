      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「いてっ」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": null,
      "idk_1": null,
      "txt": "……手を叩かれてしまった。",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "涼子",
      "idk_1": null,
      "txt": "「こういう悪い先輩の手は、こうして……こうです」",
