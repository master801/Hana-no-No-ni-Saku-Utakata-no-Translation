      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「少しでも息抜きしないと、張りつめてたままでも折れて\\nしまうって」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「少しでも息抜きしないと、張りつめてたままでも折れてしまうって」",
      "txt_idk_2": "「少しでも息抜きしないと、張りつめてたままでも折れてしまうって」"
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「肩の力をぬいて、ほら……こんな感じに」",
