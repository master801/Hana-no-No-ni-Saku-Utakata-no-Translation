      "idk_2": 456,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "涼子",
      "idk_1": null,
      "txt": "「お尻の下……硬くなっているのですが……」",
      "voices": [
