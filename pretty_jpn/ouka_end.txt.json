      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「礼なんて構わないよ」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50533"
