      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "桜花が照れ隠しに笑う。",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": null,
      "hidden_character_speaking": null,
      "txt": "こんな日がずっと続く事を祈って――二人で公園の中を\\n駆けだした。",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "こんな日がずっと続く事を祈って――二人で公園の中を駆けだした。",
      "txt_idk_2": "こんな日がずっと続く事を祈って――二人で公園の中を駆けだした。"
