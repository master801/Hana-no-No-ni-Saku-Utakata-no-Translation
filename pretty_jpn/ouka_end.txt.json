      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「あ、ああ。平気だ。何ともない……」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "actual_character_speaking": "桜花",
      "hidden_character_speaking": null,
      "txt": "「……そうか。安心した。\\n今一瞬目の色が……ああ、いや、見間違いかな」",
      "voices": [
        {
          "name": "桜花",
          "pan": 0,
          "type": 0,
          "voice": "a02_50541"
        }
      ],
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「……そうか。安心した。今一瞬目の色が……ああ、いや、見間違いかな」",
      "txt_idk_2": "「……そうか。安心した。今一瞬目の色が……ああ、いや、見間違いかな」"
    },
    {
      "actual_character_speaking": "道隆",
      "hidden_character_speaking": null,
      "txt": "「ああ、大丈夫。いきなり走ったから酸素が足りなくなった\\nのかも……」",
      "voices": null,
      "idk_1": 448,
      "scene_data": null,
      "idk_2": null,
      "txt_idk_1": "「ああ、大丈夫。いきなり走ったから酸素が足りなくなったのかも……」",
      "txt_idk_2": "「ああ、大丈夫。いきなり走ったから酸素が足りなくなったのかも……」"
    },
