      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "千歳",
      "idk_1": null,
      "txt": "「あらあら、今日は楽が出来そうかしら」",
