      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "汐音",
      "idk_1": null,
      "txt": "「お姉ちゃんは休んでてもいいよー」",
      "voices": [
        {
          "name": "汐音",
          "pan": 0,
          "type": 0,
          "voice": "a03_40474"
