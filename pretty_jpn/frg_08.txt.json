      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "麗奈",
      "idk_1": null,
      "txt": "「様子を見に来たのよ。\\n……まあ、それも要らなさそうだったけれど」",
      "voices": [
        {
          "name": "麗奈",
          "pan": 0,
          "type": 0,
          "voice": "a06_40038"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「様子を見に来たのよ。……まあ、それも要らなさそうだったけれど」",
      "txt_idk_2": "「様子を見に来たのよ。……まあ、それも要らなさそうだったけれど」"
    },
    {
      "character_speaking": "雫",
      "idk_1": null,
      "txt": "「美人のお嫁さんといちゃつくのは良いけれど、丸聞こえ\\nだったわよ」",
      "voices": [
        {
          "name": "雫",
          "pan": 0,
          "type": 0,
          "voice": "a05_40014"
        }
      ],
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "「美人のお嫁さんといちゃつくのは良いけれど、丸聞こえだったわよ」",
      "txt_idk_2": "「美人のお嫁さんといちゃつくのは良いけれど、丸聞こえだったわよ」"
    },
