      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "道隆",
      "idk_1": null,
      "txt": "「白々しいんだよっ」",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": "雫",
      "idk_1": null,
      "txt": "「新婚のうちに邪険にすると、将来的な火種になるそうよ」",
      "voices": [
        {
          "name": "雫",
