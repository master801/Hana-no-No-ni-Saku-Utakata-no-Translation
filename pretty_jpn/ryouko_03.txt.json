      "txt_idk_1": null,
      "txt_idk_2": null
    },
    {
      "character_speaking": null,
      "idk_1": null,
      "txt": "……いかんいかん。\\nなんか思考の方向が怪しくなっている。",
      "voices": null,
      "idk_2": 448,
      "scene_data": null,
      "idk_3": null,
      "txt_idk_1": "……いかんいかん。なんか思考の方向が怪しくなっている。",
